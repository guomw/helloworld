background: linear-gradient(to right,  #252525 0%,#404040 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#252525', endColorstr='#404040',GradientType=1 ); /* IE6-9 */
width: 680rpx;height: 420rpx;border-radius: 20rpx;position: relative;margin-top: 40rpx;margin:30rpx auto 10rpx auto;}
.card-active .tile .zw{width: 69rpx;height: 70rpx;position: absolute;left:56rpx;top:44rpx;display: none}
.card-not-active .tile .zw{width: 69rpx;height: 70rpx;position: absolute;left:56rpx;top:44rpx;display: block}

.card-active .tile .zw2{width: 69rpx;height: 70rpx;position: absolute;left:56rpx;top:44rpx;display: block}
.card-not-active .tile .zw2{width: 69rpx;height: 70rpx;position: absolute;left:56rpx;top:44rpx;display: none}

